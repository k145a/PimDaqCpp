#include <iostream>
#include<ctime>
using namespace std;

int main() {
	time_t t = time(0);
	struct tm * now = localtime(& t);
	cout << "Hello from PimDaqCpp " << now->tm_mon << endl; // prints HEllow from PimDaqCpp May 16, 2016
	return 0;
}
